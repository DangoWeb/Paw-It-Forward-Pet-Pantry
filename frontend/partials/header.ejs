<header>
	<div class="top"><img src="<%- vars.domain %>/paw.png">
		<% happeningNow = cms.events.sort((a, b) => a._o - b._o).find(event => ((new Date(event.startDate)) < (new Date())) && ((new Date(event.endDate)) > (new Date()))); %>
		<% if (happeningNow) { %><p>Happening Now -</p><a href="<%= happeningNow.link %>"><%= happeningNow.title %></a><% } %>
	</div>
	<div class="logo">
		<img src="<%- vars.asset_prefix %><%= cms.siteDetails[0].logo.path %>" alt="<%= cms.siteDetails[0].title %>">
		<div class="inner">
			<h1><%= cms.siteDetails[0].title %></h1>
			<h3><%= cms.siteDetails[0].description %></h3>
		</div>
	</div>
	<div class="links">
		<% cms.menu.forEach(item => { %>
		<a class="link <% if ((item.title === title) || ((item.title === 'Home') && (title === ''))) { %>active<% } %>" href="<%= item.link %>">
			<img src="<%- vars.domain %>/paw.png">
			<%= item.title %>
			<img src="<%- vars.domain %>/paw.png">
			<img src="<%- vars.domain %>/paw_theme.png" hover>
		</a>
		<% }) %>
	</div>
</header>
<main>
	<page-title><% if (title != '') { %><%- title %> | <% } %><%= cms.siteDetails[0].title %></page-title>